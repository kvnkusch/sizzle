{
  "engines": {
    "node": "16.15.0"
  },
  "name": "sizzle",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "db:clear": "docker compose down --volumes",
    "db:shell": "docker exec -ti sizzle-postgres-1 psql sizzle -U postgres",
    "db:migrations:generate": "drizzle-kit generate:pg --config=./drizzle.config.ts",
    "db:migrations:run": "ts-node ./scripts/db/run-migrations.ts",
    "db:seed": "ts-node -r tsconfig-paths/register --project ./scripts/tsconfig.json ./scripts/db/seed-dev-data.ts"
  },
  "private": true,
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@nx/esbuild": "16.5.0",
